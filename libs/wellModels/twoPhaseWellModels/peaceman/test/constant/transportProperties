FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

oil
{
    rho rho [1 -3  0 0 0] 700;
    mu  mu    [1 -1 -1 0 0] 3e-3;
}

water
{
    rho rho [1 -3  0 0 0] 1000;
    mu  mu    [1 -1 -1 0 0] 1e-3;
}

relativePermeabilityModel "BrooksCorey";

BrooksCoreyCoeffs
{
    canonicalPhase water;
    //phases         (water oil);

    water.alphaIrr 0.2;
    oil.alphaRes   0.1;
    water.m        2;
    oil.m          2;
    water.krMax    1.0;
    oil.krMax      0.9;
}

capillaryPressureModel "none";


